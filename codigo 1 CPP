/* Pseudocodigo
inicio
para i (0 hasta 10 salto 1)
si (i > 0)
mostrar (,)
mostrar (i)
mostrar (.)
fin
*/

#include <iostream>
using namespace std;
int n;
int main() {
  for (int i =0; i <=10;i++){
      if (i > 0){
        cout << ",";
       }
       cout << i;
  } cout <<".";
  
}
